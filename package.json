  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bycedric/create-issue.git"
  },
  "bugs": {
    "url": "https://github.com/bycedric/create-issue/issues"
  },
  "homepage": "https://github.com/bycedric/create-issue#readme",
  "files": [
    "build"
  ],
  "dependencies": {
    "arg": "^5.0.2"
  },
  "eslintConfig": {
    "extends": "universe/node"
  },
  "prettier": {
